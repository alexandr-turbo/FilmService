{
  "name": "film-service",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploymac": "git push origin --delete gh-pages && npm run build && git add . && git commit -m 'publish' && git subtree push --prefix dist origin gh-pages",
    "deploywin": "git push origin --delete gh-pages && npm run build && git add . && git subtree push --prefix dist origin gh-pages"
  },
  "dependencies": {
    "@trevoreyre/autocomplete-vue": "^2.2.0",
    "axios": "^0.19.0",
    "core-js": "^3.3.2",
    "gulp": "^4.0.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-slick": "^1.1.15",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "jquery": "^3.4.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "no-debugger": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "vue": {
    "publicPath": "/film-service/"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
